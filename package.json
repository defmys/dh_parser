    "@fortawesome/free-solid-svg-icons": "^5.4.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-runtime": "^6.26.0",
    "bootstrap": "^4.1.3",
    "electron": "^3.0.2",
    "jquery": "^3.3.1",
    "mirror-config-china": "^2.5.1",
    "popper.js": "^1.14.4",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
